        "favourite",
        "indexeddb",
        "loglevel",
        "memlist",
        "octicons",
        "Pathify",
        "scripthost",
        "tailwindcss",
        "uikit",
        "uniqid",
        "vuebar",
        "vuetify",
        "vuetype",
        "vuex"
